<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SciterCore.PackFolder</id>
    <version>$version$</version>
    <title>SciterCore.PackFolder</title>
    <authors>William D Cossey</authors>
    <owners>wdcossey</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">GPL-3.0-or-later</license>
    <projectUrl>https://github.com/wdcossey/SciterCore</projectUrl>
    <description>SciterCore PackFolder tasks for embedding Sciter resource files.</description>
    <copyright>© Sciter.</copyright>
    <tags>Sciter SciterCore PackFolder</tags>
    <dependencies>
      <group targetFramework="net45"/>
      <group targetFramework="netstandard2.0"/>
      <group targetFramework="xamarinmac20"/>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\_._" target="lib/net45/_._" />
    <file src="lib\_._" target="lib/netstandard2.0/_._" />
    <file src="lib\_._" target="lib/xamarinmac20/_._" />
    <file src="build\**\*.props" target="build\net45" />
    <file src="build\**\*.props" target="build\netstandard2.0" />
    <file src="build\**\*.props" target="build\xamarinmac20" />
    <file src="buildMultiTargeting\**\*.props" target="buildMultiTargeting\net45" />
    <file src="buildMultiTargeting\**\*.props" target="buildMultiTargeting\netstandard2.0" />
    <file src="buildMultiTargeting\**\*.props" target="buildMultiTargeting\xamarinmac20" />
    <file src="..\sciter-sdk\bin.win\packfolder.exe" target="scripts\packfolder.exe" />
    <file src="..\sciter-sdk\bin.osx\packfolder" target="scripts\packfolder" />
  </files>
</package>